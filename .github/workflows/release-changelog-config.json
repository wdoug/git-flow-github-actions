{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["BUG"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
        "title": "## 📦 Dependencies",
        "labels": ["dependencies"]
    },
    {
      "title": "## 📖 Docs",
      "labels": ["documentation"]
    },
    {
      "title": "## 🧹 Chores",
      "labels": ["chore"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "DESC",
  "template": "## What's Changed\n${{UNCATEGORIZED}}\n${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}}) **@${{AUTHOR}}**",
  "empty_template": "- no changes",
  "exclude_merge_branches": [
    "dev",
    "production"
  ]
}
