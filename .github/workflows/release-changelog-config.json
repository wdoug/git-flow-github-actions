{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["BUG"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
        "title": "## 📦 Dependencies",
        "labels": ["dependencies"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "DESC",
  "template": "${{CHANGELOG}}\n\n## Uncategorized Changes\n${{UNCATEGORIZED}}",
  "pr_template": "- ${{TITLE}} **(#${{NUMBER}})** ${{AUTHOR}}",
  "empty_template": "- no changes",
  "exclude_merge_branches": [
    "dev",
    "production"
  ]
}
